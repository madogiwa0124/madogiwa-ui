{
  "name": "@madogiwa-ui/vue",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/madogiwa-ui-vue.min.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/madogiwa-ui-vue.min.js",
      "default": "./dist/madogiwa-ui-vue.min.js"
    },
    "./dist/*": "./dist/*",
    "./src/*": "./src/*"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "build:demo": "vite build --mode demo",
    "preview:demo": "pnpm dlx http-server dist/demo",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "type-check": "vue-tsc --noEmit",
    "test:browser": "vitest --config=vitest.browser.config.ts"
  },
  "description": "A flexible, accessible, next-generation UI framework leveraging cutting-edge CSS features",
  "keywords": [
    "css",
    "css-framework",
    "ui-library",
    "modern-css",
    "vue"
  ],
  "author": "madogiwa0124",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/madogiwa0124/madogiwa-ui.git",
    "directory": "packages/vue"
  },
  "homepage": "https://github.com/madogiwa0124/madogiwa-ui#readme",
  "bugs": {
    "url": "https://github.com/madogiwa0124/madogiwa-ui/issues"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "@madogiwa-ui/css": "workspace:*",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/browser-playwright": "^4.0.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "postcss-preset-env": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.3",
    "vitest-browser-vue": "^2.0.0",
    "vue-tsc": "^3.1.2"
  },
  "browserslist": [
    "baseline newly available"
